<beans>
<!-- MYSQL via JNDI -->
<!-- A datasource named 'jdbc/darwino/mysql' has to be created on the server  -->
    <bean type="darwino/jsondb" name="oraclecloudJndi" class="com.darwino.config.jsonstore.JsonDbJndi" alias="demo,default"> 
        <property name="db">mysql</property>    
        <property name="jndiPath">jdbc/darwino/mysql</property>    
    </bean>
    
